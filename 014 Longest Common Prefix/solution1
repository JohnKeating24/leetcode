class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
        if(strs.size()==0) return "";
        for(int len=0;len<strs[0].size();len++){
            for(int i=1;i<strs.size();i++){
                if(strs[i][len]!=strs[0][len])
                    return strs[0].substr(0,len);
            }
        }
        return strs[0];
    }
};
